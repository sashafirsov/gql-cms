{
  "name": "@gql-cms/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run docker",
    "build": "bash bin/jwt_keys.sh && npx nx run-many --target=build --all",
    "lint": "npx nx run-many --target=lint --all",
    "test": "npx nx run-many --target=test --all",
    "e2e": "npx nx run-many --target=e2e --all",
    "affected:build": "npx nx affected --target=build",
    "affected:lint": "npx nx affected --target=lint",
    "affected:test": "npx nx affected --target=test",
    "affected:e2e": "npx nx affected --target=e2e",
    "format": "npx nx format:write",
    "format:check": "npx nx format:check",
    "update": "npx nx migrate latest",
    "docker": "docker-compose up --build -d && echo 'http://localhost:5433/status' && echo 'http://localhost:5434/graphiql' && echo 'http://localhost:4200/'",
    "serve:admin-ui:prod": "node dist/apps/admin-ui/.next/standalone/server.js",
    "admin-ui:dev": "npx nx serve @gql-cms/admin-ui",
    "gql-api:dev": "npx nx serve gql-api --output-style=stream",
    "storybook": "npx nx storybook @gql-cms/admin-ui"
  },
  "private": true,
  "dependencies": {
